KAFKA_CREATE_TOPICS: "Order:1:1:delete,Batches:1:1:delete,Fleet:1:1:delete,TripItinerary:1:1:delete"
KAFKA_LISTENER_SECURITY_PROTOCOL_MAP: PLAINTEXT:PLAINTEXT,PLAINTEXT_HOST:PLAINTEXT
